    "version": "0.2.0",
    "configurations": [
        {
            "name": "gear_freak_client",
            "cwd": "gear_freak_client",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "Launch Development",
            "cwd": "gear_freak_flutter",
            "request": "launch",
            "type": "dart",
            "args": [
                "--dart-define=ENV=dev"
            ]
        },
        {
            "name": "Launch Production",
            "cwd": "gear_freak_flutter",
            "request": "launch",
            "type": "dart",
            "args": [
                "--dart-define=ENV=prod"
            ]
        },
        {
            "name": "Launch Production (profile mode)",
            "cwd": "gear_freak_flutter",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "args": [
                "--dart-define=ENV=prod"
            ]
        },
        {
            "name": "Launch Production (release mode)",
            "cwd": "gear_freak_flutter",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "args": [
                "--dart-define=ENV=prod"
            ]
        },
        {
            "name": "gear_freak_server",
            "cwd": "gear_freak_server",
            "request": "launch",
            "type": "dart"
        }
    ]
}